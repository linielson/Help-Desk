  <div class="group">
    <%= f.label :nome, "Nome", class: :label %>
    <%= f.text_field :nome, class: :text_field, style: 'width: 300px;' %>
  </div>

  <div class="group">
    <%= f.label :cliente, "Cliente", class: :label %>
    <%= f.collection_select :cliente_id, @clientes, :id, :codigo_nome, prompt: true %>
  </div>

  <div class="group">
    <%= f.label :descricao, "Descrição", class: :label %>
    <%= f.text_area :descricao, rows: 10, class: :text_area %>
  </div>

  <div class="group">
    <%= f.label :previsao_fim, "Previsão de Término", class: :label %>
    <%= f.date_select :previsao_fim, class: :date_select %>
  </div>

  <div class="group">
    <%= f.label :gastos_previsto, "Gastos Previsto", class: :label %>
    <%= f.text_field :gastos_previsto, class: :text_field, style: 'width: 100px;' %>
    <br /><span class="description">Ex: 1000.55 </span>
  </div>

  <div class="group">
    <%= f.label :gastos_final, "Gastos Efetivos", class: :label %>
    <%= f.text_field :gastos_final, class: :text_field, style: 'width: 100px;' %>
    <br /><span class="description">Ex: 1000.55 </span>
  </div>

<div class="group navform wat-cf">
  <button class="button" type="submit">
    <%= image_tag("ok.png", alt: "Salvar") %> Salvar
  </button>
  <%= link_to "Cancelar", projetos_path, class: "text_button_padding link_button" %>
</div>
